{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "rootDir": "./",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["node", "express"],
    "noImplicitAny": false,
    "sourceMap": true,
      "ts-node":{
        "esm": true
      },
  },
  "include": ["src/ai.ts"],
  "exclude": ["node_modules", "dist"]
}

